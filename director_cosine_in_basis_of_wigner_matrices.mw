<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">#-------------------------------------------------------------#
#---- All definition are taken from Zare Book ----------------#
#-------------------------------------------------------------#


#-------------------------------------------------------------#
# Product of two Wigner Matrices -----------------------------#
#-------------------------------------------------------------#
ProdWigner:=proc(j1,m1,k1,j2,m2,k2)
    local djmk,i,n,m3,k3;
    n:=0;
    m3:=-m1-m2;
    k3:=-k1-k2;
    djmk:=0;
    for i from abs(j1-j2) by 1 to j1+j2 do
        n:=n+1;
        #print(n);
        if i &gt;= abs(m3) and i &gt;=abs(k3) then
            djmk:=(2*i+1)*S3J(j1,j2,i,m1,m2,m3)*S3J(j1,j2,i,k1,k2,k3)*D_(i,m3,k3)+djmk;
        end if
    end do;
    return djmk;
end proc:
#-------------------------------------------------------------#


#-------------------------------------------------------------#
# Director cosine as function of Wigner matrices -------------#
#-------------------------------------------------------------#
CosDir:=proc(n)   <Font encoding="UTF-8">#---- notation : D_(j,m,k) /!\134 -------------#</Font>
    local COS;
    if n=1 then   #---- cos xX
        COS:=1/2*(D_(1,1,1)-D_(1,1,-1)-D_(1,-1,1)+D_(1,-1,-1));
    elif n=2 then #---- cos xY
        COS:=I/2*(D_(1,1,1)-D_(1,1,-1)+D_(1,-1,1)-D_(1,-1,-1));
    elif n=3 then #---- cos xZ
        COS:=1/sqrt(2)*(D_(1,0,-1)-D_(1,0,1));
    elif n=4 then #---- cos yX
        COS:=I/2*(-D_(1,1,1)-D_(1,1,-1)+D_(1,-1,1)+D_(1,-1,-1));
    elif n=5 then #---- cos yY
        COS:=1/2*(D_(1,1,1)+D_(1,1,-1)+D_(1,-1,1)+D_(1,-1,-1));
    elif n=6 then #---- cos yZ
        COS:=I/sqrt(2)*(D_(1,0,-1)+D_(1,0,1));
    elif n=7 then #---- cos zX
        COS:=1/sqrt(2)*(D_(1,-1,0)-D_(1,1,0));
    elif n=8 then #---- cos zY
        COS:=-I/sqrt(2)*(D_(1,-1,0)+D_(1,1,0));
    elif n=9 then #---- cos zZ
        COS:=D_(1,0,0);
    else
        error &quot;n should be in [1,9]&quot;;
    end if;
    return COS
end proc:
#-------------------------------------------------------------#


#-------------------------------------------------------------#
# Product of two sum of Wigner Matrices ----------------------#
#-------------------------------------------------------------#
ProdSumWigner:=proc(A,B)
    local i1,i2,n1,n2,j1,m1,k1,j2,m2,k2,C,c1,c2,a,b;
    a:=indets(A,function);
    n1:=nops(a);
    b:=indets(B,function);
    n2:=nops(b);
    C:=0;
    for i1 from 1 by 1 to n1 do
        j1:=op(1,a[i1]);
        m1:=op(2,a[i1]);
        k1:=op(3,a[i1]);
        c1:=coeff(A,a[i1]);
        for i2 from 1 by 1 to n2 do
            j2:=op(1,b[i2]);
            m2:=op(2,b[i2]);
            k2:=op(3,b[i2]);
            c2:=coeff(B,b[i2]);
            C:=C+c1*c2*ProdWigner(j1,m1,k1,j2,m2,k2);
        end do;
    end do;
end proc:
#-------------------------------------------------------------#



#-------------------------------------------------------------#
# Make wigner matrix (J,M,K) ---------------------------------#
#-------------------------------------------------------------#
MakeWigner:=proc(J,M,K)
    local C;
    C:=D_(J,M,K);
    return C;
end proc:
#-------------------------------------------------------------#

